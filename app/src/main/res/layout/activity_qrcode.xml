<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.liu.eparty.activity.meeting.QrcodeActivity">

    <ImageView
        android:id="@+id/sign_in_qrcode"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView63"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="16dp"
        android:text="@string/scan_qrcode"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/sign_in_qrcode"
        app:layout_constraintStart_toStartOf="@+id/sign_in_qrcode"
        app:layout_constraintTop_toBottomOf="@+id/sign_in_qrcode" />


</android.support.constraint.ConstraintLayout>
